<h1>User#index</h1>
<p><%= @msg %></p>
<a href="/user/add">Add</a>
<a href="/report/">Report</a>
<a href="/user/find">Find</a>
<table>
	<tr>
		<th>Id</th><th>Name</th><th>Division</th><th><th></th></th>
	</tr>
	<% @data.each do |obj| %>
	<tr>
		<td><%= obj.id %></td>
		<td><a href="/user/<%= obj.id %>"><%= obj.name %></a></td>
		<td><%= obj.division.name %></td>
		<td><a href="/user/edit/<%= obj.id %>">Edit</a></td>
		<td><a href="javascript:delData(<%= obj.id %>);">Delete</a></td>
	</tr>
	<% end %>
</table>
<script>
function delData(num){
	if (confirm('このデータを削除しますか？')){
		document.location = "/user/delete/" + num;
	}
	return false;
}
</script>
